<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout" xmlns="sap.m"
	xmlns:fb="sap.ui.comp.filterbar">
	<TableSelectDialog id="idApprVH" noDataText="No Data" title="Requestor" search="onSearchAppr" confirm="onApprVHConfirm"
		cancel="onApprVHConfirm" showClearButton="true" items="{ path : '/UserSet', sorter : { path : 'Bname', descending : false }}"
		contentWidth="30%">
		<columns>
			<Column minScreenWidth="Tablet" demandPopin="true">
				<Text text="Name"/>
			</Column>
			<Column minScreenWidth="Tablet" demandPopin="true">
				<Text text="First Name"/>
			</Column>
			<Column minScreenWidth="Tablet" demandPopin="true">
				<Text text="Last Name"/>
			</Column>
		</columns>
		<ColumnListItem>
			<Text text="{Bname}"/>
			<Text text="{NameFirst}"/>
			<Text text="{NameLast}"/>
		</ColumnListItem>
	</TableSelectDialog>
</core:FragmentDefinition>
