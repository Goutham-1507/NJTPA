<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout" xmlns="sap.m"
	xmlns:fb="sap.ui.comp.filterbar">
	<TableSelectDialog id="idUserVH" noDataText="No Data" title="Responsible Contacts" search="onSearchUser" confirm="onUserVHConfirm1"
		cancel="onUserVHConfirm1" showClearButton="true" items="{ path : '/UserSet', sorter : { path : 'Bname', descending : false }}"
		contentWidth="30%">
		<columns>
			<Column minScreenWidth="Tablet" demandPopin="true">
				<Text text="Name"/>
			</Column>
			<Column minScreenWidth="Tablet" demandPopin="true">
				<Text text="First Name"/>
			</Column>
			<Column minScreenWidth="Tablet" demandPopin="true">
				<Text text="Last Name"/>
			</Column>
			<Column minScreenWidth="Tablet" demandPopin="true" visible="false">
				<Text text="Email"/>
			</Column>
			<!--<Column minScreenWidth="Tablet" demandPopin="true">-->
			<!--	<Text text="Cell"/>-->
			<!--</Column>-->
			<Column minScreenWidth="Tablet" demandPopin="true" visible="false">
				<Text text="Telephone"/>
			</Column>
		</columns>
		<ColumnListItem>
			<Text text="{Bname}"/>
			<Text text="{NameFirst}"/>
			<Text text="{NameLast}"/>
			<Text text="{SmtpAddr}"/>
			<!--<Text text="{Cell}"/>-->
			<Text text="{TelNumber}"/>
		</ColumnListItem>
	</TableSelectDialog>
</core:FragmentDefinition>
